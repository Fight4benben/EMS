<div class="container-fluid main-top">
    <div class="count-info-te compare-add">
        <div class=" build-info count-info">
            <div class="main-menu">
                <hr>
                <i class="fa fa-paste"></i>筛选
            </div>
            <div class="row">
                <div class="select-width">建筑名称：</div>
                <div class="filter-calc">
                    <form role="form">
                        <div class="form-group">
                            <select class="form-control" id="buildinglist"></select>
                        </div>
                    </form>
                </div>
            </div>
            <div class="row">
                <div class="select-width" style="margin-top: 5px;">能耗类别：</div>
                <div id="se_CompareBtns" class="filter-calc">
                    <acronym title="电">
                        <button class="btn btn-elc"
                                style="width: 20px; height: 20px;" type="button"></button>
                    </acronym> <acronym title="水">
                        <button class="btn btn-water"
                                style="width: 20px; height: 20px;" type="button"></button>
                    </acronym> <acronym title="光伏"><button class="btn btn-solar" type="button"></button></acronym>
                </div>
            </div>
            <div class="row">
                <div class="select-width"><p>年 份：</p></div>
                <div class="filter-calc">
                    <div id="calendar" class="input-group date form_date" data-date="" data-date-format="dd MM yyyy" data-link-field="dtp_input2" data-link-format="yyyy-mm-dd">
                        <input id="calendarbox" class="form-control" size="16" type="text" value="" readonly>
                        <span class="input-group-addon"><span class="glyphicon glyphicon-calendar"></span></span>
                    </div>
                </div>
            </div>
            <div class="row dis-tree">
                <div class="count-list" style="margin-top: 7px;">
                    <p>
                        <img src="/app/img/count3.png">分项名称：
                    </p>
                    <div>
                        <div id="treeview"></div>
                    </div>
                    
                </div>
            </div>
        </div>
    </div>
    <div class="count-info-te-calc">
        <div id="content">
            <div class="cp-compareanalysis">
                <div class="count-control">
                    <div id="compareChart" style="height:300px;"></div>
                </div>
                <div data-spy="scroll" data-target="#navbarExample" data-offset="0"
                     class="scrollspy-example" style="height:100%;width: 100%;overflow-x: auto;">
                    <table id="comparetable"
                           class="table table-striped table-bordered table-hover count-table">
                        <thead>
                            <tr>
                                <th style="background: #0076D0;">日期</th>
                                <th style="background: #0076D0;">本期电量(kW·h)</th>
                                <th style="background: #0076D0;">去年同期电量(kW·h)</th>
                                <th style="background: #0076D0;">同比(%)</th>
                            </tr>
                        </thead>
                        <tbody></tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="/app/js/Item/Compare.js" type="text/javascript"></script>